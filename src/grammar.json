    },
    "_single_quote_str_char": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_escape_sequence"
        },
        {
          "type": "PATTERN",
          "value": "[^'\\r\\n]"
