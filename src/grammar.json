              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_primary_expression"
                },
                {
                  "type": "SYMBOL",
                  "name": "postfix_operation"
                }
              ]
            }
          }
        ]
      }
    },
    "math_operation": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "left",
            "content": {
              "type": "SYMBOL",
