                "value": ")"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_pairwise_operation"
        }
      ]
    },
    "single_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "_primary_expression"
        },
        {
          "type": "SYMBOL",
          "name": "assignment_operation"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "&"
            },
            {
              "type": "SYMBOL",
              "name": "_primary_expression"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "prefix_operation"
        },
        {
          "type": "SYMBOL",
          "name": "postfix_operation"
        }
      ]
    },
    "expression_sequence": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "single_expression"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "single_expression"
              }
            ]
          }
        }
      ]
    },
    "variable_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "scope_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "assignment_operation": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "left",
            "content": {
              "type": "SYMBOL",
              "name": "single_expression"
            }
          },
          {
            "type": "SYMBOL",
            "name": "assignment_operator"
          },
          {
            "type": "FIELD",
            "name": "right",
            "content": {
              "type": "SYMBOL",
              "name": "single_expression"
            }
          }
        ]
      }
    },
    "_pairwise_operation": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "math_operation"
        },
        {
          "type": "SYMBOL",
          "name": "comparison_operation"
        },
        {
          "type": "SYMBOL",
          "name": "bitwise_operation"
        }
      ]
    },
    "postfix_operation": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "operand",
            "content": {
              "type": "SYMBOL",
              "name": "_primary_expression"
            }
          },
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "++"
                },
                {
                  "type": "STRING",
                  "value": "--"
                }
              ]
            }
          }
        ]
      }
    },
    "prefix_operation": {
      "type": "PREC_RIGHT",
      "value": 1,
      "content": {
        "type": "SEQ",
