        },
        {
          "type": "SYMBOL",
          "name": "unset"
        }
      ]
    },
    "numeric_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[+-]?([0-9])"
        },
        {
          "type": "PATTERN",
          "value": "[+-]?([0-9]*[.])?[0-9]+"
        },
        {
          "type": "PATTERN",
          "value": "0[xX][0-9a-fA-F]+"
        }
      ]
    },
    "boolean_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 5,
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[tT][rR][uU][eE]"
            },
            {
              "type": "PATTERN",
              "value": "[fF][aA][lL][sS][eE]"
            }
          ]
        }
      }
    },
    "string_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_double_quote_str_char"
              }
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_single_quote_str_char"
              }
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    },
    "_escape_sequence": {
      "type": "PATTERN",
      "value": "`[^\\r\\n\\t]"
    },
    "_double_quote_str_char": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_escape_sequence"
        },
        {
          "type": "PATTERN",
          "value": "[^\"\\r\\n]"
        }
